{
  "name": "macflix",
  "version": "0.1.0",
  "description": "Macflix is Netflix on your Mac",
  "productName": "Macflix",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron src/main/main.js",
    "package": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/netflix.icns --prune=true --out=builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tommoor/macflix.git"
  },
  "keywords": [
    "electron",
    "desktop",
    "netflix",
    "video"
  ],
  "author": "Tom Moor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tommoor/macflix/issues"
  },
  "dependencies": {
    "jquery": "^2.1.4",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "electron-builder": "^17.5.0",
    "electron-prebuilt": "^0.36.0"
  }
}
